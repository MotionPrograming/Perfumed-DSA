#include <bits/stdc++.h>
using namespace std;
vector<int>v;

int lowerBoundBinarrySearch(int l,int r,int x){
  int ans = -1;// if not exist
  
  while(l<=r){
    int mid = (l+r)/2 ;
    if(v[mid] >= x){
      ans = mid;
      r = mid-1;
    }else {
      l = mid+1;
    }
  }
  return ans;
}
int upperBoundBinarrySearch(int l,int r,int x){
  int ans = -1;// if not exist
  
  while(l<=r){
    int mid = (l+r)/2 ;
    if(v[mid] >= x){
      ans = mid;
      r = mid-1;
    }else {
      l = mid+1;
    }
  }
  return ans;
}
int main() 
{   
    int n,q;
    cin>>n>>q;
    v.resize(n);
    
    for(int i = 0;i<n;i++){cin>>v[i];}
    
    while(q--){
      int x;
      cin>>x;
      
    int result1 =  lowerBoundBinarrySearch(0,n-1,x);
    cout<<result1<<endl;
    cout<<endl;
    int result2 =  upperBoundBinarrySearch(0,n-1,x);
    cout<<result2<<endl;
    }
    return 0;
}
